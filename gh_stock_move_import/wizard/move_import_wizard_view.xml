<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_move_import_wizard" model="ir.ui.view">
            <field name="name">Import Picking</field>
            <field name="model">move.import.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Import Picking">
                    <field name="state" invisible="1"/>
                    <group colspan="4" col="4">
                        <group col="2">
                            <field name="date_picking" states="step1" required="1"/>
                        </group>
                        <group col="4">
                            <field name="data" colspan="4" states="step1"/>
                        </group>
                    </group>
                    <group colspan="4" col="4">
                        <field name="wizard_line" states="step2" colspan="4" nolabel="1">
                            <form string="Picking Line">
                                <separator string="General" colspan="4"/>
                                <field name="sequence" required="1" readonly="1"/>
                                <field name="date_picking" required="1"/>
                                <newline/>
                                <group colspan="2" col="2">
                                    <separator string="Product Information" colspan="2"/>
                                    <field name="default_code" required="1"/>
                                    <field name="product_id" required="1"/>
                                    <field name="qty_available" required="1"/>
                                    <field name="lot_name" required="1"/>
                                </group>
                                <group colspan="2" col="2">
                                    <separator string="Purchase Information" colspan="2"/>
                                    <field name="order_id" readonly="1"/>
                                    <field name="order_line_id" required="1"/>
                                    <newline/>
                                    <separator string="Location information" colspan="2"/>
                                    <field name="origin" invisible="1"/>
                                    <field name="destination"/>
                                </group>
                            </form>
                            <tree string="Picking Lines">
                                <field name="sequence" readonly="1"/>
                                <field name="date_picking"/>
                                <field name="destination"/>
                                <field name="default_code"/>
                                <field name="product_id"/>
                                <field name="qty_available"/>
                                <field name="lot_name"/>
                                <field name="order_line_id"/>
                            </tree>
                        </field>
                    </group>
                    <group colspan="8" col="8">
                        <separator string="" colspan="8"/>
                        <label colspan="6" width="220"/>
                        <button special="cancel" string="_Close" icon="gtk-cancel" states="step1"/>
                        <button name="import_excel" states="step1" string="_Import" type="object" icon="gtk-go-forward"/>
                        <button name="back_step1" states="step2" string="_Back" type="object" icon="gtk-ok"/>
                        <button name="create_picking" states="step2" string="_Create" type="object" icon="gtk-convert"/>
                    </group>
                </form>
            </field>
        </record>
        <record id="action_view_move_import_wizard" model="ir.actions.act_window">
            <field name="name">Import Picking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">move.import.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <menuitem
        action="action_view_move_import_wizard"
        id="menu_view_move_import_wizard"
        parent="stock.menu_stock_warehouse_mgmt"/>
    </data>
</openerp>